# Source the environment
# Order matters.  ohmyzshell is last so that its inherence bash completions support (e.g. compinit) runs
# after earlier customizations have the opportunity to modify the fpath or do other related tasks.
for file in ~/.config/scruth_env/.{aliases,functions,exports,extras,ohmyzshell}; do
  [ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;